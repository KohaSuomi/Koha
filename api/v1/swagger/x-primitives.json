{
  "batchOverlayReportContainerId": {
    "type": "integer"
  },
  "batchOverlayReportId": {
    "type": "integer"
  },
  "biblionumber": {
    "type": "integer",
    "description": "internally assigned biblio identifier"
  },
  "borrowernumber": {
    "type": "integer",
    "description": "internally assigned user identifier"
  },
  "branchcode": {
    "type": "string",
    "description": "a unique key assigned to each branch"
  },
  "cardnumber": {
    "type": ["string", "null"],
    "description": "library assigned user identifier"
  },
  "cityid": {
    "type": "integer",
    "description": "internally assigned city identifier",
    "readOnly": true
  },
  "email": {
    "type": ["string", "null"],
    "description": "primary email address for patron's primary address"
  },
  "firstname": {
    "type": ["string", "null"],
    "description": "patron's first name"
  },
  "identifier": {
    "type": "integer",
    "description": "Internal identifier"
  },
  "itemnumber": {
    "type": ["integer", "null"],
    "description": "internally assigned item identifier"
  },
  "barcode": {
    "type": ["string", "null"],
    "description": "item barcode"
  },
  "marcxml": {
    "type": "string",
    "description": "MARCXML serialized into a form variable. Swagger2-spec forbids sending pure XML objects in the body so we work around the limitation"
  },
  "phone": {
    "type": ["string", "null"],
    "description": "primary phone number for patron's primary address"
  },
  "reserve_id": {
    "type": "integer",
    "description": "Internal hold identifier"
  },
  "surname": {
    "type": "string",
    "description": "patron's last name"
  },
  "article_request_id": {
    "type": "integer",
    "description": "Internal article request identifier"
  }
}
